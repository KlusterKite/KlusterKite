<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
  <appSettings>
    <add key="nodeManagerUrl" value="http://entry/api/1.x/clusterkit/nodemanager/getConfiguration"/>
    <add key="workingDirectory" value="/opt/node"/>
    <add key="stopMode" value="CleanRestart"/>
    <add key="stopAction" value="echo stopped"/>
    <add key="containerType" value="publisher"/>
    <add key="fallbackConfiguration" value="fallBackConfiguration.json"/>
  </appSettings>
</configuration>
